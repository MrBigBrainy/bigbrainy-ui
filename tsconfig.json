{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",

    "lib": ["DOM", "ESNext"],

    "jsx": "react-jsx",

    "moduleResolution": "Node",

    "strict": true,
    "declaration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,

    "types": ["react", "react-dom"]
  },
  "include": ["src"]
}
